{
  "name": "@ruizalexandre/mithril-utils",
  "version": "0.0.8",
  "description": "Some Mithril.js utils",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A lib",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "mithriljs",
    "mithril",
    "javascript",
    "js"
  ],
  "author": "Alexandre RUIZ",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:ruizalexandre/mithril-utils.git"
  },
  "devDependencies": {
    "@types/mithril": "^2.2.7",
    "prettier": "^3.5.3",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "mithril": "^2.2.15"
  },
  "publishConfig": {
    "access": "public"
  }
}
